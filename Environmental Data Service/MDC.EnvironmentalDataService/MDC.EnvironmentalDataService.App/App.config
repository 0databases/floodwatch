<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <!-- For more information on Entity Framework configuration, visit http://go.microsoft.com/fwlink/?LinkID=237468 -->
    <section name="entityFramework" type="System.Data.Entity.Internal.ConfigFile.EntityFrameworkSection, EntityFramework, Version=6.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" requirePermission="false" />
  </configSections>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
  </startup>
  <connectionStrings>
    <add name="Hilltop" connectionString="Data Source=SERVER;Initial Catalog=DB; Integrated Security=True" providerName="System.Data.SqlClient" />
  </connectionStrings>
  <appSettings>
    <add key="GifGenUrl" value="http://HILLTOP_SERVER/data.hts?Service=Hilltop&amp;Request=Gifgen" />
    <add key="HillTopServerUrl" value="http://HILLTOP_SERVER/data.hts?Service=Hilltop&amp;Request=GetData&amp;Site={0}&amp;Measurement={1}&amp;TimeInterval={2}" />
    <add key="FeedFileBaseLocation" value="C:\Hilltop\EnvironmentalDataService" />
    <add key="FeedUrl" value="EXTERNAL_URL_FEED_AVAILABLE_AT" />
    <add key="GifGenFileExtension" value="png" />
    <add key="GifGenTraceColour" value="Blue" />
    <add key="GifGenTraceThickness" value="2" />
    <add key="GifGenFontType" value="Verdana" />
    <add key="GifGenFWatchWidth" value="575" />
    <add key="GifGenFWatchHeight" value="460" />
    <add key="GifGenBWaterWidth" value="250" />
    <add key="GifGenBWaterHeight" value="200" />
  </appSettings>
  <entityFramework>
    <defaultConnectionFactory type="System.Data.Entity.Infrastructure.SqlConnectionFactory, EntityFramework" />
    <providers>
      <provider invariantName="System.Data.SqlClient" type="System.Data.Entity.SqlServer.SqlProviderServices, EntityFramework.SqlServer" />
    </providers>
  </entityFramework>
</configuration>