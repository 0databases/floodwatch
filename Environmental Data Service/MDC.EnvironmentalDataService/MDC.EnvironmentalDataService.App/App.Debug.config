<?xml version="1.0"?>
<!-- For more information on using app.config transformation visit http://go.microsoft.com/fwlink/?LinkId=125889 -->
<configuration xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">
  <connectionStrings>
    <add name="Hilltop" connectionString="Data Source=SERVER;Initial Catalog=DB; Integrated Security=True" providerName="System.Data.SqlClient" xdt:Transform="SetAttributes" xdt:Locator="Match(name)" />
  </connectionStrings>
  <appSettings>
    <add key="FeedFileBaseLocation" value="C:\inetpub\wwwroot\EnvironmentalDataService" xdt:Transform="SetAttributes" xdt:Locator="Match(key)" />
    <add key="FeedUrl" value="EXTERNAL_URL_FEED_AVAILABLE_AT" xdt:Transform="SetAttributes" xdt:Locator="Match(key)" />
  </appSettings>
</configuration>